#!/usr/bin/env node

var program = require('commander')
var Stack = require('../lib/stack')
var async = require('async');
// options

program
  //.usage('viking build [scripts] [styles] [files]')
  .usage('')
 
program.on('--help', function(){
  console.log('  Examples:');
  console.log();
  console.log('    # re-generate an etcd token for etcd cluster');
  console.log('    $ viking seed');
  console.log();
  process.exit();
});

var async = require('async');
var utils = require('component-consoler');
var path = require('path');
var fs = require('fs');
var token = require('../lib/tools/etcdtoken')
var log = utils.log;
var config = require('../lib/config')();

token(function(err, token){
  fs.writeFileSync(config.network.tokenpath, token, 'utf8')
})