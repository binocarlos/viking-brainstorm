#!/usr/bin/env node
var program = require('commander')
var etcd = require('../lib/bootstrap/etcd')

program
  .usage('')
  .option('-s, --seed', 'generate a new etcd cluster token')

program.parse(process.argv);

if(program.seed){
	etcd.writeToken(function(err, token){
		if(err){
			console.error(err)
			process.exit(1)
		}
		else{
			console.log('token: ' + token)
		}
	})
}
