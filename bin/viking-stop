#!/usr/bin/env node

var program = require('commander');

program
  .usage('')
 
program.on('--help', function(){
  console.log('  Examples:');
  console.log();
  console.log('    # stop viking');
  console.log('    $ viking stop');
  console.log();
  console.log();
  console.log();
  process.exit();
});

// parse argv

program.parse(process.argv);

var utils = require('component-consoler');
var path = require('path');
var fs = require('fs');
var log = utils.log;

var Config = require('../lib/config')
var config = Config();





